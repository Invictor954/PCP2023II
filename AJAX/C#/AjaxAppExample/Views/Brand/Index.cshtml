@model List<Brand>


<h1>Brand</h1>

<table class="table">
    <thread>
        <tr>
            <th>Name</th>
        </tr>
    </thread>
    <tbody>
        @foreach(var brand in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m=>brand.Name)</td>
            </tr>
        }
    </tbody>
</table>